{
  "name": "next-multilingual",
  "description": "An opinionated end-to-end multilingual solution for Next.js.",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./router": "./lib/router/index.js",
    "./link": "./lib/link/index.js",
    "./head": "./lib/head/index.js"
  },
  "typesVersions": {
    "*": {
      "router": [
        "lib/router/index.d.ts"
      ],
      "link": [
        "lib/link/index.d.ts"
      ],
      "head": [
        "lib/head/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=13"
  },
  "watch": {
    "build": {
      "patterns": "*",
      "extensions": "*.ts?(x)",
      "ignore": "node_modules"
    }
  },
  "scripts": {
    "build_watch": "npm-watch",
    "build": "rm -Rf ./lib && tsc",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "dot-properties": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "next": "^10.1.3",
    "npm-watch": "^0.9.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "next": "^10",
    "react": "^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
